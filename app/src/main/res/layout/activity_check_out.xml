<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".checkOut">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        android:background="#092C71"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="4dp"
            android:layout_marginBottom="6dp"
            app:layout_constraintBottom_toTopOf="@+id/pricetxt"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/logo" />

        <TextView
            android:id="@+id/textView7"
            style="@style/fontStyle"
            android:layout_width="104dp"
            android:layout_height="29dp"
            android:layout_marginStart="3dp"
            android:layout_marginEnd="4dp"
            android:layout_marginBottom="12dp"
            android:text="Name "
            android:textSize="24dp"
            app:layout_constraintBottom_toTopOf="@+id/Nametxt"
            app:layout_constraintEnd_toEndOf="@+id/textView6"
            app:layout_constraintStart_toStartOf="@+id/Nametxt"
            app:layout_constraintTop_toBottomOf="@+id/pricetxt" />

        <TextView
            android:id="@+id/textView4"
            style="@style/fontStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:layout_marginBottom="16dp"
            android:text="Phone"
            android:textSize="22dp"
            app:layout_constraintBottom_toTopOf="@+id/Phonetxt"
            app:layout_constraintStart_toStartOf="@+id/Nametxt"
            app:layout_constraintTop_toBottomOf="@+id/Nametxt" />

        <TextView
            android:id="@+id/textView6"
            style="@style/fontStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:layout_marginBottom="7dp"
            android:text="Location"
            android:textSize="22dp"
            app:layout_constraintBottom_toTopOf="@+id/locationtxt"
            app:layout_constraintStart_toStartOf="@+id/locationtxt"
            app:layout_constraintTop_toBottomOf="@+id/Phonetxt" />

        <EditText
            android:id="@+id/Nametxt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="16dp"
            android:ems="10"
            android:hint="Enter your name"
            android:inputType="text"
            app:layout_constraintBottom_toTopOf="@+id/textView4"
            app:layout_constraintEnd_toEndOf="@+id/Phonetxt"
            app:layout_constraintStart_toStartOf="@+id/Phonetxt"
            app:layout_constraintTop_toBottomOf="@+id/textView7" />

        <EditText
            android:id="@+id/locationtxt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="33dp"
            android:layout_marginBottom="13dp"
            android:ems="10"
            android:hint="Enter your Location"
            android:inputType="text"
            app:layout_constraintBottom_toTopOf="@+id/donebtn"
            app:layout_constraintEnd_toEndOf="@+id/donebtn"
            app:layout_constraintTop_toBottomOf="@+id/textView6" />

        <EditText
            android:id="@+id/Phonetxt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:ems="10"
            android:hint="Enter your Phone"
            android:inputType="phone"
            app:layout_constraintBottom_toTopOf="@+id/textView6"
            app:layout_constraintEnd_toEndOf="@+id/locationtxt"
            app:layout_constraintStart_toStartOf="@+id/locationtxt"
            app:layout_constraintTop_toBottomOf="@+id/textView4" />

        <Button
            android:id="@+id/donebtn"
            style="@style/buttonstyles2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="7dp"
            android:layout_marginBottom="23dp"
            android:text="Done"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/cancelbtn"
            app:layout_constraintTop_toBottomOf="@+id/locationtxt" />

        <Button
            android:id="@+id/cancelbtn"
            style="@style/buttonstyles2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="24dp"
            android:layout_marginBottom="23dp"
            android:text="Cansel"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/donebtn"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/pricetxt"
            style="@style/fontStyle"
            android:layout_width="279dp"
            android:layout_height="40dp"
            android:layout_marginStart="54dp"
            android:layout_marginBottom="14dp"
            android:text="Total Price : "
            android:textSize="25dp"
            app:layout_constraintBottom_toTopOf="@+id/textView7"
            app:layout_constraintStart_toStartOf="@+id/imageView3"
            app:layout_constraintTop_toBottomOf="@+id/imageView3" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>